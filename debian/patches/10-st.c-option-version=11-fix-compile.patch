From 887d2bbf1c5c10eed1352ffe11ead6560ae0087a Mon Sep 17 00:00:00 2001
From: Jari Aalto <jari.aalto@cante.net>
Date: Sat, 19 Dec 2009 23:01:15 +0200
Subject: [PATCH] st.c: Fix compile errors


Signed-off-by: Jari Aalto <jari.aalto@cante.net>
---
 st.c |    6 +++---
 1 files changed, 3 insertions(+), 3 deletions(-)

diff --git a/st.c b/st.c
index 196efcb..4e4973e 100644
--- a/st.c
+++ b/st.c
@@ -1046,7 +1046,7 @@ kpress(XEvent *ev) {
 	shift = e->state & ShiftMask;
 	len = XLookupString(e, buf, sizeof(buf), &ksym, NULL);
 
-	if(customkey = kmap(ksym))
+	if( (customkey = kmap(ksym)) )
 		ttywrite(customkey, strlen(customkey));
 	else if(len > 0) {
 		buf[sizeof(buf)-1] = '\0';
@@ -1113,8 +1113,8 @@ run(void) {
 
 int
 main(int argc, char *argv[]) {
-    if(argc == 2 && (!strncmp("-V", argv[1], 3)) ||
-                    (!strncmp("--version", argv[1], 8)) )
+    if(argc == 2 && ((!strncmp("-V", argv[1], 3)) ||
+                     (!strncmp("--version", argv[1], 8))) )
 		die("st-" VERSION ", Â© 2009 st engineers\n");
 	else if(argc != 1)
 		die("usage: st [-V]\n");
-- 
